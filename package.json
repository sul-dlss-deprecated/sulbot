{
  "name": "sulbot",
  "version": "0.0.0",
  "private": true,
  "author": "Erin Fahy <efahy@stanford.edu>",
  "description": "A robot for heaven deploys",
  "scripts": {
    "pm2:start": "pm2 start bin/hubot --name sulbot --interpreter bash  -l sulbot.log -- -a slack -d",
    "pm2:restart": "pm2 stop sulbot && pm2 delete sulbot && npm run pm2:start",
    "pm2": "if pm2 list | grep sulbot; then npm run pm2:restart; else npm run pm2:start; fi"
  },
  "dependencies": {
    "hubot": "^2.18.0",
    "hubot-auto-deploy": "^0.1.9",
    "hubot-deploy": "^0.12.12",
    "hubot-diagnostics": "0.0.1",
    "hubot-google-images": "^0.2.6",
    "hubot-google-translate": "^0.2.0",
    "hubot-help": "^0.1.3",
    "hubot-maps": "0.0.2",
    "hubot-pugme": "^0.1.0",
    "hubot-redis-brain": "0.0.3",
    "hubot-rules": "^0.1.1",
    "hubot-scripts": "^2.16.2",
    "hubot-shipit": "^0.2.0",
    "hubot-slack": "^3.4.2",
    "pm2": "^1.1.3"
  },
  "engines": {
    "node": "0.10.x"
  }
}
